<!DOCTYPE html>
<html>
	<head>
		<title>{{ title }}</title>
		<link rel='stylesheet' href='/stylesheets/style.css' />
	</head>
	<body>
		<div class="container">
            
            <!-- navigation -->
			<nav>
				<div class="content">
					<a href="/" class="logo">
						<img src="/images/logo.png" alt="FastDelivery">
					</a>
                    <ul class="menu">
                        <li><a href="/">Calculation</a></li>
                        <li><a href="/packages">Package Management</a></li>
                        <li><a href="/trucks">Truck Management</a></li>
                        <li><a href="/storages">Storage Management</a></li>
                    </ul>
				</div>
			</nav>
            
            <!-- management page section -->
            <section class="management">
                <div class="content">
                    <div class="center-box">
                        <div class="row">
                            <div>
                                <h1><span class="magenta">{{{model_form_header}}}</span></h1>
                                <br/>
                                {{{model_form_template}}}
                                <br/>
                            </div>
                        </div>
                        <div class="row">
                            <div>
                                <h1><span class="magenta">{{{content_header}}}</span></h1>
                                <br/>
                                <table class="content_table, container">
                                    <thead>
                                      <tr>
                                        {{{content_table_header}}}
                                      </tr>
                                    </thead>
                                    <tbody>
                                        {{{content_table_row}}}
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </section>

            <!-- footer -->
            <div class="footer">
                <div class="content">
                    fast delivery &copy; 2013
                </div>
            </div>
            
		</div>
	</body>
</html>