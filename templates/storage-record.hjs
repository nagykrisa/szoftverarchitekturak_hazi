<form action="/storages" method="post" enctype="multipart/form-data">
	<input type="hidden" name="formsubmitted" value="yes">
	<input type="hidden" name="_id" value="{{_id}}">
	Name:<br />
	<input type="text" name="name" value="{{name}}" required/><br />
    Longitude:<br />
	<input type="number" name="longitude" value="{{longitude}}" step= any required/><br />
    Latitude:<br />
	<input type="number" name="latitude" value="{{latitude}}" step= any required/><br /><br />
	<input type="submit" value="submit" />
</form>